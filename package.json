{
  "name": "photography-website",
  "version": "1.0.0",
  "description": "Professional photography website built with Next.js",
  "main": "index.js",
  "scripts": {
    "dev": "next dev -p 3100",
    "dev:stable": "node scripts/dev-stable.js",
    "dev:clean": "./scripts/cleanup.sh && next dev -p 3100",
    "build": "next build",
    "start": "next start -p 3100",
    "lint": "next lint",
    "clean": "./scripts/cleanup.sh",
    "logs": "tail -f logs/dev-server.log",
    "logs:error": "tail -f logs/dev-server-errors.log"
  },
  "keywords": ["photography", "nextjs", "website", "portfolio"],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.15.0",
  "dependencies": {
    "@tailwindcss/postcss": "4.1.0",
    "autoprefixer": "10.4.21",
    "gray-matter": "4.0.3",
    "lucide-react": "0.540.0",
    "next": "15.5.0",
    "next-seo": "6.8.0",
    "postcss": "8.5.6",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "tailwindcss": "4.1.0",
    "typescript": "5.9.2"
  },
  "devDependencies": {
    "@types/node": "24.3.0",
    "@types/react": "19.0.2",
    "@types/react-dom": "19.0.2",
    "eslint": "9.33.0",
    "eslint-config-next": "15.5.0",
    "prettier": "3.6.2"
  }
}
